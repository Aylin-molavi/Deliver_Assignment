    "#revc\n",
    "from Bio.Seq import Seq\n",
    "def reversedDNA (dna):\n",
    "    if dna == \"\":\n",
    "        return \"\"\n",
    "    else:\n",
    "        return dna.reverse_complement(inplace=False)\n",
    "    \n",
    "dna = Seq(\"AAAACCCGGT\")\n",
    "print(reversedDNA(dna))    "
